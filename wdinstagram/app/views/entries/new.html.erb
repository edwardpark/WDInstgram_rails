<h1>Enter New Photo</h1>
<%= form_for :entry, url: entries_path do |f| %>
  <p>
    <%= f.label :author %><br>
    <%= f.text_field :author %>
  </p>

  <p>
    <%= f.label :photo_url %><br>
    <%= f.text_field :photo_url %>
  </p>
  <p>
    <%= f.label :date_taken %><br>
    <%= f.text_field :date_taken %>
  </p>
  <p>
    <%= f.submit "Enter New Photo! " %>
  </p>
<% end %>
